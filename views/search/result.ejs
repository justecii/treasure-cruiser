<div class="mainBody col-xs-9">
  <form action="/search/result" method="POST">
    <div class="form-group">
      <label for="name">Card Name</label>
      <input id="name" class="form-control" type="text" name="name" placeholder="Search Card">
      <input class="btn btn-primary" type="submit">
    </div>
  </form>
  <% results.forEach(function(results){ %>
    <% if (results.layout === "normal") { %>
    <div class="well">
      <div class="cardDisplay">
        <img class="cardThumb"src="<%= results.image_uris.normal%>">
      </div>
      <div class="cardDisplay">
        <h2 class="capitalize"><%= results.name %></h2>
        <p>Code: <%= results.set%></p>
        <p>Set Name: <%= results.set_name%></p>
        <p class="capitalized">Rarity: <%= results.rarity %></p>
        <div class="prices">
          <p>Price: $<%= results.usd%></p>
        </div>
      </div>
      <div class="cardDisplay">
        <form method="POST" action="/collection">
          <input hidden type="text" name="name" value="<%= results.name%>">
          <input hidden type ="text" name="img" value="<%= results.image_uris.small %>>">
          <input hidden type ="text" name="bigImg" value="<%= results.image_uris.large%>">
          <input hidden type ="text" name="setName" value="<%= results.set_name%>">
          <input hidden type ="text" name="multiverseId" value="<%= results.multiverse_id %>">
          <input hidden type ="text" name="rarity" value="<%= results.rarity%>">
          <input hidden type="text" name="price" value="<%= results.usd %>">
          <button class="btn btn-primary" type="submit">Add to Collection</button>
        </form>
      </div>
    </div>
    <% } else { %>
      <%= console.log(results.card_faces[0].image_uris.normal) %>
      <div class="well">
        <div class="cardDisplay">
          <img class="cardThumb"src="<%= results.card_faces[0].image_uris.normal%>">
        </div>
        <div class="cardDisplay">
          <h2 class="capitalize"><%= results.name %></h2>
          <p>Code: <%= results.set%></p>
          <p>Set Name: <%= results.set_name%></p>
          <p class="capitalized">Rarity: <%= results.rarity %></p>
          <div class="prices">
            <p>Price: $<%= results.usd%></p>
          </div>
        </div>
        <div class="cardDisplay">
          <form method="POST" action="/collection">
            <input hidden type="text" name="name" value="<%= results.name%>">
            <input hidden type ="text" name="img" value="<%= results.card_faces[0].image_uris.normal %>>">
            <input hidden type ="text" name="bigImg" value="<%= results.card_faces[0].image_uris.normal%>">
            <input hidden type ="text" name="setName" value="<%= results.set_name%>">
            <input hidden type ="text" name="multiverseId" value="<%= results.multiverse_id %>">
            <input hidden type ="text" name="rarity" value="<%= results.rarity%>">
            <input hidden type="text" name="price" value="<%= results.usd %>">
            <button class="btn btn-primary" type="submit">Add to Collection</button>
          </form>
        </div>
      </div>
    <% } %>
  <% }); %>
</div>
<div class="col-xs-3 sidebar">
  <h2>Quick Links:</h2>
</div>
