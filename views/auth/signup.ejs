<form action="/auth/signup" method="POST">
  <div class="form-group">
    <label for="authEmail">Email</label>
    <input id="authEmail" class="form-control" type="email" name="email">
  </div>

  <div class="form-group">
    <label for="authName">User Name</label>
    <input id="authName" class="form-control" type="text" name="profileName">
  </div>
  <div class="form-group">
    <label for="authPassword">Password</label>
    <input id="authPassword" class="form-control" type="password" name="password">
  </div>
  <div class="form-group">
    <label for="authFirst">First name</label>
    <input id="authFirst" class="form-control" type="text" name="firstName">
  </div>
  <div class="form-group">
    <label for="authLast">Last name</label>
    <input id="authLast" class="form-control" type="text" name="lastName">
  </div>
  <div class="form-group">
    <label for="authLocation">Location</label>
    <input id="authLocation" class="form-control" type="text" name="location">
  </div>


  <input class="btn btn-primary" type="submit">
</form>
